{
    "manifest_version": 3,
    "name": "UNDUCK",
    "version": "0.1",
    "description": "text filter boi",
    "action": 
        {
            "default_popup": "Testing/test.html",
            "default_icon": "images/bookmarksbar.png"
        },
    "icons": 
        {
            "16":"images/icon16.png",
            "48":"images/icon48.png",
            "128":"images/icon128.png"
        },
    "content_scripts":
    [
        {   
            "matches": ["<all_urls>", "*://*/*"],
            "js": ["Testing/test.js"],
            "all_frames": true,
            "run_at": "document_end"
        }
    ],
    "permissions":["storage"],
    "commands":{},
    "host_permissions": 
    [
        "*://*/*"
    ]
}